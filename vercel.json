{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "frontend",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/css/(.*)", "dest": "/frontend/css/$1" },
    { "src": "/js/(.*)", "dest": "/frontend/js/$1" },
    { "src": "/assets/(.*)", "dest": "/frontend/assets/$1" },
    { "src": "/research.html", "dest": "/frontend/research.html" },
    { "src": "/deposit-new.html", "dest": "/frontend/deposit-new.html" },
    { "src": "/vault-new.html", "dest": "/frontend/vault-new.html" },
    { "src": "/dashboard.html", "dest": "/frontend/dashboard.html" },
    { "src": "/", "dest": "/frontend/index.html" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
